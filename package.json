{
  "name": "dynamodb-fast-access",
  "version": "1.0.2",
  "description": "Default CRUD operations for managing AWS DynamoDB table items in an easy-to-extend structure.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "tsc && nyc --reporter=lcov --reporter=text mocha",
    "startLocalDB": "java -Djava.library.path=./res/DynamoDBLocal/DynamoDBLocal_lib -jar res/DynamoDBLocal/DynamoDBLocal.jar -sharedDb -inMemory -port 3000 &",
    "build": "tsc",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrian-mezei/dynamodb-fast-access.git"
  },
  "keywords": [
    "DynamoDB",
    "CRUD",
    "fast",
    "access",
    "AWS",
    "database",
    "NoSQL",
    "Node.js",
    "Amazon"
  ],
  "author": "Adri√°n Mezei",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "bugs": {
    "url": "https://github.com/adrian-mezei/dynamodb-fast-access/issues"
  },
  "homepage": "https://github.com/adrian-mezei/dynamodb-fast-access#readme",
  "dependencies": {
    "aws-sdk": "^2.539.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.3",
    "@types/istanbul": "^0.4.30",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
